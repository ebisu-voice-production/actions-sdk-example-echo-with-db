{
  "name": "actions-sdk-example-echo-with-db",
  "version": "1.0.0",
  "description": "An Example of Actions on Google by Firebase Cloud Functions and Realtime Database",
  "main": "src/index.js",
  "scripts": {
    "firebase": "firebase",
    "test": "npm run eslint",
    "eslint": "eslint src",
    "deploy": "firebase deploy --only functions",
    "deploy-action": "gactions update --action_package action.json --project ${PROJECT}"
  },
  "dependencies": {
    "actions-on-google": "^2.3.0",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "firebase-tools": "^4.2.1"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2015
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "off"
    }
  },
  "keywords": [
    "Actions on Google",
    "Cloud Functions for Firebase",
    "Firebase Realtime Database"
  ],
  "license": "MIT"
}
